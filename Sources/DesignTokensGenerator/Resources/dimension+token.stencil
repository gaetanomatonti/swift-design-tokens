public struct DimensionToken {
  let value: CGFloat
}

extension {{tokenType}} {
{% if tokens.count > 0 %}

  // MARK: - Tokens

{% call tokensBlock tokens %}
{% endif %}
{% if aliases.count > 0 %}

  // MARK: - Aliases

{% call aliasesBlock aliases tokenType %}
{% endif %}
}

#if canImport(Foundation)
import Foundation

public extension CGFloat {
  static func token(dimension: DimensionToken) -> CGFloat {
    dimension.value
  }
}
#endif
